{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": [{
    "target": "icpcsec",
    "public": "public",
    "ignore": [".dev/**", "**/.*", "**/node_modules/**"],
    "headers": [
      {
        "source": "**",
        "headers": [{ "key": "Cache-Control", "value": "no-cache" }]
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }, {
    "target": "icpcasia",
    "public": "public",
    "ignore": [".dev/**", "**/.*", "**/node_modules/**"],
    "headers": [
      {
        "source": "**",
        "headers": [{ "key": "Cache-Control", "value": "no-cache" }]
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }]
}
